{
	// cSpell:disable
	"coc.preferences.hoverTarget": "preview",
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.snippetStatusText": "Ⓢ ",
	"coc.preferences.extensionUpdateCheck": "weekly",
	// use colorizer
	"coc.preferences.colorSupport": false,
	"suggest.detailMaxLength": 60,
	"suggest.noselect": false,
	"suggest.snippetIndicator": "~",
	"suggest.acceptSuggestionOnCommitCharacter": true,
	"suggest.enablePreselect": false,
	"suggest.triggerAfterInsertEnter": false,
	"suggest.timeout": 10000,
	"suggest.enablePreview":true,
	"suggest.floatEnable": true,
	"suggest.minTriggerInputLength": 1,
	"suggest.detailField": "preview",
	"suggest.triggerCompletionWait": 100,
	"suggest.echodocSupport": true,
	"suggest.completionItemKindLabels": {
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},
	"codeLens.enable":true,
	"codeLens.separator":"‣",
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.virtualText": true,
	"diagnostic.signOffset": 1,
	"diagnostic.errorSign": "✘",
	"diagnostic.warningSign": "⚡",
	"diagnostic.enable": true,
	"diagnostic.infoSign": "ღ",
	"diagnostic.hintSign": "ஐ",
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.enableSign": true,
	"diagnostic.refreshAfterSave": true,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextLines": 4,
	"diagnostic.virtualTextCurrentLineOnly": true,
	"diagnostic.maxWindowHeight":8,
	"diagnostic.maxWindowWidth":60,
	"snippets.priority": 100,
	"snippets.enableStatusItem": true,
	"snippets.autoTrigger": true,
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips"
	],
	"git.enableGutters": true,
	"git.virtualTextPrefix": " ❯❯❯ ",
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"git.branchCharacter":"",
	"git.addedSign.text":"→",
	"git.changedSign.text":"⇆",
	"git.removedSign.text":"←",
	"git.topRemovedSign.text": "↵",
	"git.changeRemovedSign.text": "↹",
	"git.enableGlobalStatus": true,
	"git.signOffset": 2,
	"java.home": "/Library/Java/JavaVirtualMachines/jdk-14.jdk/Contents/Home",
	"java.format.enabled": true,
	"java.completion.enabled": true,
	"java.format.onType.enabled": true,
	"java.completion.importOrder": [
		"java",
		"javax",
		"com",
		"org"
	],
	"xml.java.home": "/Library/Java/JavaVirtualMachines/jdk-14.jdk/Contents/Home",
	"xml.codeLens.enabled": false,
	"xml.format.enabled": true,
	"xml.completion.autoCloseTags": true,
	"xml.format.quotations": "doubleQuotes",
	"xml.format.splitAttributes": false,
	"python.pythonPath": "python",
	"python.analysis.downloadChannel": "daily",
	"python.autoUpdateLanguageServer": true,
	"python.linting.pylintUseMinimalCheckers": true,
	"python.linting.pylintPath": "pylint",
	"python.linting.enabled":true,
	"python.linting.flake8CategorySeverity.E":"Error",
	"python.linting.flake8CategorySeverity.F":"Information",
	"python.linting.flake8CategorySeverity.W":"Warning",
	"python.linting.flake8Enabled":true,
	"python.linting.flake8Path": "flake8",
	"python.workspaceSymbols.enabled": true,
	"python.pipenvPath": "pipenv",
	"python.autoComplete.addBrackets": true,
	"python.jediEnabled": false,
	"python.analysis.diagnosticEnabled":true,
	"python.analysis.diagnosticPublishDelay":100,
	"python.formatting.provider": "yapf",
	"python.formatting.yapfArgs": [
        "--style",
        "{SPACES_AROUND_POWER_OPERATOR: True, SPACES_BEFORE_COMMENT: 4, SPLIT_BEFORE_LOGICAL_OPERATOR: True}"
    ],
	"json.format.enable":true,
	"json.enable": true,
	"css.enable": true,
	"tailwindCSS.htmlLanguages": [
		"blade",
		"edge",
		"eelixir",
		"ejs",
		"elixir",
		"elm",
		"erb",
		"eruby",
		"haml",
		"handlebars",
		"html",
		"HTML (EEx)",
		"HTML (Eex)",
		"html.twig",
		"jade",
		"leaf",
		"markdown",
		"njk",
		"nunjucks",
		"php",
		"razor",
		"slim",
		"svelte",
		"twig",
		"vue"
	],
	"html.enable": true,
	"html.format.indentInnerHtml": true,
	"html.autoClosingTags": true,
	"html.format.enable": true,
	"html.suggest.html5": true,
	"html.filetypes": [
		"html",
		"handlebars",
		"htmldjango",
		"jsp"
	],
	"javascript.showUnused": true,
	"tsserver.npm": "/usr/local/bin/npm",
	"javascript.suggest.names": true,
	"javascript.referencesCodeLens.enable": true,
	"javascript.suggest.enabled": true,
	"javascript.suggestionActions.enabled": true,
	"latex.lint.onSave": true,
	"rust-analyzer.serverPath": "/usr/local/Cellar/rust-analyzer/2020-09-07/bin/rust-analyzer",
	"signature.target":"float",
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"html",
		"css",
		"python",
		"dart",
		"go",
		"php",
		"sh",
		"md"
	],
	"cmake.cmakePath": "cmake",
	"tsserver.trace.server":"verbose",
	"explorer.width": 30,
	"explorer.file.column.indent.indentLine": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.file.column.git.showIgnored": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.enableVimDevicons": true,
	"translator.toLang":"zh",
	"translator.maxsize": 5000,
	"translator.engines": [
		"bing",
		"google",
		"youdao",
		"haici"
	],
	"python.autoComplete.extraPaths": [
		"${env:SPARK_HOME}/python",
		"${env:SPARK_HOME}/python/pyspark"
	],
	"http.proxy": "http://127.0.0.1:7890",
	"languageserver": {
		"foo": {
			"module": "/usr/local/lib/node_modules/foo/index.js",
			"args": ["--node-ipc"],
			"filetypes": ["foo"],
			"trace.server": "verbose",
			"rootPatterns": ["root.yml"],
			"execArgv": ["--nolazy", "--inspect-brk=6045"],
			"initializationOptions": {
			},
			"settings": {
				"validate": true
			}
		},
		"intelephense": {
			"command": "intelephense",
			"args": ["--stdio"],
			"filetypes": ["php"],
			"initializationOptions": {
				"storagePath": "/tmp/intelephense"
				}
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cc", "c++", "cpp", "objc", "objcpp"],
			"rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
			"initializationOptions": {
			"cache": {
				"directory": "/tmp/ccls"
			},
			"clang": {
				// make sure you have installed commandLineTools
				"resourceDir": "/Library/Developer/CommandLineTools/usr/lib/clang/12.0.0",
				"extraArgs": [
					"-isystem",
					"/Applications/Xcode-beta.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
					"-I",
					"/Applications/Xcode-beta.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/usr/include"
				]
				}
			}
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"],
			"initializationOptions": {
				"gocodeCompletionEnabled": true,
				"diagnosticsEnabled": true,
				"lintTool": "golint"
			}
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		},
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": ["~"]
		},
		"sql": {
			"command": "sql-language-server",
			"args" : ["up", "--method", "stdio"],
			"filetypes": ["sql", "mysql"]
		 }
  }
}
