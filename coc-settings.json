		"nunjucks",
		"php",
		"razor",
		"slim",
		"svelte",
		"twig",
		"vue"
	],
	"html.enable": true,
	"html.format.indentInnerHtml": true,
	"html.autoClosingTags": true,
	"html.format.enable": true,
	"html.suggest.html5": true,
	"html.filetypes": [
		"html",
		"handlebars",
		"htmldjango",
		"jsp"
	],
	"javascript.showUnused": true,
	"tsserver.npm": "/usr/local/bin/npm",
	"javascript.suggest.names": true,
	"javascript.referencesCodeLens.enable": true,
	"javascript.suggest.enabled": true,
	"javascript.suggestionActions.enabled": true,
	"latex.lint.onSave": true,
	"signature.target":"float",
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"html",
		"css",
		"wxml",
		"wxss",
		"python",
		"dart",
		"go",
		"php",
		"sh",
		"md"
	],
	"cmake.cmakePath": "cmake",
	"tsserver.trace.server":"verbose",
	"explorer.width": 30,
	"explorer.file.column.indent.indentLine": true,
	"explorer.file.showHiddenFiles": true,
	"explorer.git.showIgnored": true,
	"explorer.filename.colored.enable": true,
	"explorer.focus": false,
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.enableVimDevicons": true,
	"explorer.previewAction.onHover": "labeling",
	"translator.toLang":"zh",
	"translator.engines": [
		"bing",
		"google",
		"youdao"
	],
	"rust-analyzer.enable": true,
	"rust-analyzer.server.path": "/usr/bin/rust-analyzer",
	"languageserver": {
		"dart": {
			"command": "dart",
			"args": [
				"/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
				"--lsp",
				"--client-id",
				"vim",
				"--client-version",
				"coc.nvim"
			],
			"filetypes": ["dart"],
			"trace.server": "verbose"
		},
		"foo": {
			"module": "/usr/local/lib/node_modules/foo/index.js",
			"args": ["--node-ipc"],
			"filetypes": ["foo"],
			"trace.server": "verbose",
			"rootPatterns": ["root.yml"],
			"execArgv": ["--nolazy", "--inspect-brk=6045"],
			"initializationOptions": {
			},
			"settings": {
				"validate": true
			}
		},
		"intelephense": {
			"command": "intelephense",
			"args": ["--stdio"],
			"filetypes": ["php"],
			"initializationOptions": {
				"storagePath": "/tmp/intelephense"
			}
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "objc", "objcpp"],
			"rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				},
				"clang": {
					// make sure you have installed commandLineTools
					"resourceDir": "/usr/local/llvm11/lib/clang/11.0.0",
					"extraArgs": [
						"-isystem",
						"/usr/local/llvm11/include/c++/v1"
					]
				}
			}
		},
		"cmake": {
			"command": "cmake-language-server",
			"filetypes": ["cmake"],
			"rootPatterns": [
				"build/"
			],
			"initializationOptions": {
				"buildDirectory": "build"
			}
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"disableWorkspaceFolders": true,
			"filetypes": ["go"]
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		},
		"sql": {
			"command": "sql-language-server",
			"args" : ["up", "--method", "stdio"],
			"filetypes": ["sql", "mysql"]
		},
		"latex": {
			"command": "/PATH/TO/texlab",
			// not start server at home directory, since it won't work.
			"ignoredRootPaths": ["~"],
			"filetypes": ["tex", "bib", "plaintex", "context"]
		}
	}
}
