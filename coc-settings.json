{
	"coc.preferences.extensionUpdateCheck": "weekly",
	// use colorizer
	"suggest.noselect": false,
	"suggest.snippetIndicator": "~",
	"suggest.acceptSuggestionOnCommitCharacter": true,
	"suggest.enablePreselect": false,
	"suggest.triggerAfterInsertEnter": false,
	"suggest.timeout": 10000,
	"suggest.minTriggerInputLength": 1,
	"suggest.detailField": "preview",
	"suggest.completionItemKindLabels": {
		"class": "\uf0e8",
		"color": "\ue22b",
		"constant": "\uf8fe",
		"default": "\uf29c",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"event": "\ufacd",
		"field": "\uf93d",
		"file": "\uf723",
		"folder": "\uf115",
		"function": "\u0192",
		"interface": "\uf417",
		"keyword": "\uf1de",
		"method": "\uf6a6",
		"module": "\uf40d",
		"operator": "\uf915",
		"property": "\ue624",
		"reference": "\ufa46",
		"snippet": "\ue60b",
		"struct": "\ufb44",
		"text": "\ue612",
		"typeParameter": "\uf728",
		"unit": "\uf475",
		"value": "\uf89f",
		"variable": "\ue71b"
	},
	"codeLens.enable":true,
	// "codeLens.separator":"✀ ",
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.virtualText": true,
	"diagnostic.errorSign": "✘",
	"diagnostic.warningSign": "⚡",
	"diagnostic.enable": true,
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.enableSign": true,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextLines": 4,
	"diagnostic.virtualTextCurrentLineOnly": true,
	"diagnostic.messageDelay": 100,
	"snippets.priority": 100,
	"snippets.autoTrigger": true,
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips"
	],
	"git.enableGutters": true,
	"git.virtualTextPrefix": " ❯❯❯ ",
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"git.branchCharacter":"",
	"git.addedSign.text":"→",
	"git.changedSign.text":"⇆",
	"git.removedSign.text":"←",
	"git.topRemovedSign.text": "↵",
	"git.changeRemovedSign.text": "↹ ",
	"git.enableGlobalStatus": true,
//	"java.home": null,
//	"java.format.enabled": true,
//	"java.completion.enabled": true,
//	"java.format.onType.enabled": true,
//	"java.completion.importOrder": [
//		"java",
//		"javax",
//		"com",
//		"org"
//	],
	"xml.java.home": null,
	"xml.codeLens.enabled": false,
	"xml.format.enabled": true,
	"xml.completion.autoCloseTags": true,
	"xml.format.quotations": "doubleQuotes",
	"xml.format.splitAttributes": false,
	"python.pythonPath": "python",
	"python.analysis.stubPath": "typings",
	"python.linting.enabled":true,
	"python.linting.flake8Enabled": true,
	"python.linting.flake8CategorySeverity.E": "Warning",
	"python.linting.flake8CategorySeverity.F": "Information",
	"python.linting.flake8CategorySeverity.W": "Warning",
	"python.formatting.provider": "yapf",
	// autopep8
	"python.formatting.autopep8Path": "/usr/local/bin/autopep8",
	"python.formatting.yapfPath": "/usr/local/bin/yapf",
	"json.format.enable":true,
	"json.enable": true,
	"tailwindCSS.htmlLanguages": [
		"blade",
		"edge",
		"eelixir",
		"ejs",
		"elixir",
		"elm",
		"erb",
		"eruby",
		"haml",
		"handlebars",
		"html",
		"HTML (EEx)",
		"HTML (Eex)",
		"html.twig",
		"jade",
		"leaf",
		"markdown",
		"njk",
		"nunjucks",
		"php",
		"razor",
		"slim",
		"svelte",
		"twig",
		"vue"
	],
	"html.format.indentInnerHtml": true,
	"html.autoClosingTags": true,
	"html.format.enable": true,
	"html.suggest.html5": true,
	"html.filetypes": [
		"html",
		"handlebars",
		"htmldjango",
		"jsp"
	],
	"javascript.showUnused": true,
	"tsserver.npm": "npm",
	"javascript.suggest.names": true,
	"javascript.suggest.enabled": true,
	"javascript.suggestionActions.enabled": true,
	"signature.target":"float",
	"cmake.cmakePath": "cmake",
	"tsserver.trace.server":"verbose",
	"explorer.width": 30,
	"explorer.file.column.indent.indentLine": false,
	"explorer.file.showHiddenFiles": true,
	"explorer.git.showIgnored": true,
	"explorer.filename.colored.enable": true,
	"explorer.focus": false,
	"explorer.icon.enableNerdfont": true,
	"translator.toLang":"zh",
	"translator.engines": [
		"bing",
		"google",
		"youdao"
	],
	"rust-analyzer.enable": true,
	"rust-analyzer.server.path": "rust-analyzer",
	"languageserver": {
		"dart": {
			"command": "dart",
			"args": [
				"/usr/local/opt/dart/libexec/bin/snapshots/analysis_server.dart.snapshot",
				"--lsp",
				"--client-id",
				"vim",
				"--client-version",
				"coc.nvim"
			],
			"filetypes": ["dart"],
			"trace.server": "verbose"
		},
		"foo": {
			"module": "/usr/local/lib/node_modules/foo/index.js",
			"args": ["--node-ipc"],
			"filetypes": ["foo"],
			"trace.server": "verbose",
			"rootPatterns": ["root.yml"],
			"execArgv": ["--nolazy", "--inspect-brk=6045"],
			"initializationOptions": {
			},
			"settings": {
				"validate": true
			}
		},
		"intelephense": {
			"command": "intelephense",
			"args": ["--stdio"],
			"filetypes": ["php"],
			"initializationOptions": {
				"storagePath": "/tmp/intelephense"
			}
		},
		"cmake": {
			"command": "cmake-language-server",
			"filetypes": ["cmake"],
			"rootPatterns": [
				"build/"
			],
			"initializationOptions": {
				"buildDirectory": "build"
			}
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"]
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": ["dockerfile"],
			"args": ["--stdio"]
		},
		"sql": {
			"command": "sql-language-server",
			"args" : ["up", "--method", "stdio"],
			"filetypes": ["sql", "mysql"]
		},
		"latex": {
			"command": "texlab",
			// not start server at home directory, since it won't work.
			"ignoredRootPaths": ["~"],
			"filetypes": ["tex", "bib", "plaintex", "context"]
		}
  },
  "snippets.ultisnips.pythonPrompt": false
}
